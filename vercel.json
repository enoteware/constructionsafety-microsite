{
  "rewrites": [
    {
      "source": "/ssho-services",
      "destination": "/ssho-services.html"
    },
    {
      "source": "/safety-representatives",
      "destination": "/safety-representatives.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
